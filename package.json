{
  "name": "logexplorer",
  "displayName": "Log Explorer",
  "description": "A Custon Viewer to explore .log files",
  "version": "0.4.0",
  "publisher": "FlavioLeal",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:logExplorer.test"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "customEditors": [
      {
        "viewType": "logExplorer.test",
        "displayName": "Log Explorer",
        "selector": [
          {
            "filenamePattern": "*.log"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": {
      "title": "LogExplorer",
      "properties": {
        "logexplorer.ColumnsList": {
          "type": "array",
          "default": [
            "message",
            "level"
          ],
          "description": "List of columns rendered"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "rollup": "^2.62.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-preprocess": "^4.10.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@tsconfig/svelte": "^3.0.0",
    "node.js": "^0.0.1-security",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlaviovLeal/LogExplorer"
  }
}
